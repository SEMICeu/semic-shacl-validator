<http://data.europa.eu/it6/codelist_imports>
  rdf:type owl:Ontology ;
  owl:imports <https://raw.githubusercontent.com/SEMICeu/MLDCAT-AP/refs/heads/main/releases/codelist/dataset-status.ttl> ;
  owl:imports  <https://raw.githubusercontent.com/SEMICeu/MLDCAT-AP/refs/heads/main/releases/codelist/flow-status.ttl>
.

:DatasetStatusRestriction
    a sh:NodeShape ;
    rdfs:comment "Dataset Status Restriction" ;
    rdfs:label "Dataset Status Restriction" ;
    sh:property [
        sh:hasValue <http://openml.org/openml/dataset-status> ;
        sh:minCount 1 ;
        sh:nodeKind sh:IRI ;
        sh:path skos:inScheme
    ] .	

:FlowStatusRestriction
    a sh:NodeShape ;
    rdfs:comment "Flow Status Restriction" ;
    rdfs:label "Flow Status Restriction" ;
    sh:property [
        sh:hasValue <http://openml.org/openml/flow-status> ;
        sh:minCount 1 ;
        sh:nodeKind sh:IRI ;
        sh:path skos:inScheme
    ] .	
	
:Dataset_ShapeCV
    a sh:NodeShape ;
    sh:property [
        sh:node :DatasetStatusRestriction ;
        sh:nodeKind sh:IRI ;
        sh:path adms:status ;
	    sh:description "Multiple status can be used but it is recommended to also provide at least one concept of <http://openml.org/openml/dataset-status> should be present" ;
        sh:severity sh:Warning
    ] ;
    sh:targetClass dcat:Dataset.

:Flow_ShapeCV
    a sh:NodeShape ;
    sh:property [
        sh:node :FlowStatusRestriction ;
        sh:nodeKind sh:IRI ;
        sh:path adms:status ;
	    sh:description "Multiple status can be used but it is recommended to also provide at least one concept of <http://openml.org/openml/flow-status> should be present" ;
        sh:severity sh:Warning
    ] ;
    sh:targetClass it6:Flow.